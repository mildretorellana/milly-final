{% extends '_layouts/member-base.njk' %}

{% block content %}
<div class="tourdates" class="table-responsive">
  <div class="tour-main">
    <p>Queen World Tour Dates</p>
  </div>       
  <table class="table table-hover">
    <thead class="tour-head">
      <tr>
        <th><b>Date</b></th>
        <th><b>City</b></th>
        <th><b>Venue</b></th>
        <th><b>Tickets</b></th>
      </tr>
    </thead>
    {% for tourdates in tourdates.tourdates %}
    <tbody class="tour-body">
      <tr>
        <td>{{ tourdates.date }}</td>
        <td>{{ tourdates.city }}</td>
        <td>{{ tourdates.venue }}</td>
        <td><a class="btn btn-dark" href="..." role="button">On Sale</a></td>
      </tr>
    </tbody>
    {% endfor %}
  </table>
</div>

{% endblock %}
